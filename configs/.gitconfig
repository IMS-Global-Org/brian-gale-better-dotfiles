[user]
	name = BrennickL
	email = brennick.sci@gmail.com

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  ca = checkout -a
  cb = checkout -b
  df = diff
  lg = log -p
  po = push origin
	hs = log --pretty='%C(yellow)%h %C(cyan)%cd %Cblue%aN%C(auto)%d %Creset%s' --graph --date=relative --date-order
[push]
	default = simple
[core]
	excludesfile = /Users/brennicklangston/.gitignore_global
[commit]
	gpgsign = false
[gpg]
	program = gpg
[pull]
	rebase = true
[init]
	defaultBranch = master
[diff]
	tool = nvimdiff
[merge]
	tool = nvimdiff
[difftool]
	prompt = false
